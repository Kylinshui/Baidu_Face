<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bshui.baidu_face">

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FeatureSettingActivity"
            android:label="@string/title_activity_feature_setting"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ImageMatchImageActivity" />
        <activity android:name=".RgbDetectActivity" />
        <activity android:name=".UserGroupManagerActivity" />
        <activity android:name=".GroupListActivity" />
        <activity android:name=".UserListActivity" />
        <activity android:name=".UserActivity" />
        <activity android:name=".AddGroupActivity" />
        <activity android:name=".RegActivity" />
        <activity
            android:name=".RgbVideoIdentityActivity"
            android:label="@string/title_activity_rgb_video_identity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>